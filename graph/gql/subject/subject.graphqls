type Subject {
    Id: ID! @goField(name: "Id")
    CreatedAt: DateTime!
    UpdatedAt: DateTime!
    DeletedAt: DateTime
    EducationLevelId: ID! @goField(name: "EducationLevelId")
    Name: String!
}

input SubjectInput {
    EducationLevelId: Int @goField(name: "EducationLevelId")
    Name: String
}