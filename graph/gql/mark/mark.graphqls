type Mark {
    Id: ID! @goField(name: "Id")
    CreatedAt: DateTime!
    UpdatedAt: DateTime!
    DeletedAt: DateTime
    UserId : ID! @goField(name: "UserId")
    AuthorId: ID! @goField(name: "AuthorId")
    AuthorComment: String!
    AuthorMark: Int!
}

input MarkInput {
    UserId : Int @goField(name: "UserId")
    AuthorId: Int @goField(name: "AuthorId")
    AuthorComment: String
    AuthorMark: Int
}
